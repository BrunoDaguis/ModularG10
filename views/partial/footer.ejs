<script>
	$(function(){
		$("#formRegister").submit(function(e){
			e.preventDefault();

			register();
		});
	})
	function login(){
        var obj = {
            email: $("#email").val(),
            password: $("#password").val()
        }

        ajax.send({
            url: "/api/user/login",
            data: JSON.stringify(obj),
            method: "POST",
            beforeSend: function () {

            },
            success: function (response) {
                location.reload();
            },
            complete: function () {

            },
            error: function (error) {
            	alert('Email/Senha Inválidos');
            }
        });

        return false;
	}

	function register(){
        var obj = {
        	name: $("#nameRegister").val(),
            email: $("#emailRegister").val(),
            password: $("#passwordRegister").val(),
            dateBirth: $("#yearBirth option:selected").val() + '-' + $("#monthBirth option:selected").val() + '-' + $("#dayBirth option:selected").val()
        }

        ajax.send({
            url: "/api/user",
            data: JSON.stringify(obj),
            method: "POST",
            beforeSend: function () {

            },
            success: function (response) {
                location.href = '/';
            },
            complete: function () {

            },
            error: function (error) {
            	alert('Erro no cadastro');
            }
        });
	}
</script>